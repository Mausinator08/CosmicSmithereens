{
	"C_Cpp.errorSquiggles": "Enabled",
	"cmake.configureOnOpen": true,
	"C_Cpp.intelliSenseEngine": "Default",
	"cmake.debugConfig": {
		"externalConsole": false,
		"logging": {
			"programOutput": true
		}
	},
	"testMate.cpp.test.workingDirectory": "${workspaceFolder}",
	"testMate.cpp.test.executables": "{build,Build,BUILD,out,Out,OUT}/**/{test,Test,TEST}*.exe",
	"testMate.cpp.discovery.gracePeriodForMissing": 10,
	"testMate.cpp.discovery.strictPattern": true,
	"cmake.buildTask": true,
	"cmake.setBuildTypeOnMultiConfig": true,
	"files.associations": {
		"array": "cpp",
		"deque": "cpp",
		"list": "cpp",
		"vector": "cpp",
		"string_view": "cpp",
		"ranges": "cpp"
	}
}
