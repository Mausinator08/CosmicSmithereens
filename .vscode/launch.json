	"version": "0.2.0",
	"configurations": [
		{
			"name": "(Windows) Launch Debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/bin/Debug/CosmicSmithereens.exe",
			"preLaunchTask": "Install Debug",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/build/bin/Debug/",
			"environment": [],
			"internalConsoleOptions": "openOnSessionStart",
			"avoidWindowsConsoleRedirection": true,
			"externalConsole": false,
			"miDebuggerPath": "G:/msys64/mingw64/bin/gdb.exe",
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "(Windows) Launch Release",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/bin/Release/CosmicSmithereens.exe",
